  "authors": [
    "gudenau"
  ],
  "contact": {
    "homepage": "https://github.com/gudenau/minecraft-realip/",
    "sources": "https://github.com/gudenau/minecraft-realip/"
  },

  "license": "CC0-1.0",

  "environment": "server",
  "entrypoints": {
